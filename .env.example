# Supabase Configuration
SUPABASE_URL=your_supabase_project_url
SUPABASE_ANON_KEY=your_supabase_anon_key
SUPABASE_SERVICE_ROLE_KEY=your_service_role_key

# OpenAI Configuration (for GPT models and DALL-E)
OPENAI_API_KEY=your_openai_api_key

# Google Search Configuration
GOOGLE_API_KEY=your_google_api_key
GOOGLE_SEARCH_ENGINE_ID=your_search_engine_id

# Image Generation Configuration
IMAGE_GENERATION_PROVIDER=placeholder  # Options: placeholder, stable_diffusion, dall_e, midjourney
STABILITY_API_KEY=your_stability_api_key  # For Stable Diffusion
# MIDJOURNEY_API_KEY=your_midjourney_key  # For Midjourney (when available)

# Database Configuration (optional, uses Supabase by default)
DATABASE_URL=postgresql://user:password@host:port/database

# Environment
ENVIRONMENT=development  # Options: development, staging, production